{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "eofline": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-empty": false,
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}